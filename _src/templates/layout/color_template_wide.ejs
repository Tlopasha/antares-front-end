<!-- Preloader -->

<style>
    [data-preload] {
        opacity: 0;
    }

    .grid-stack-item-content {
        background: #fff;
    }

    aside.main-sidebar {
        width: 230px;
        height: 100vh;
    }
    @media only screen and (min-width: 1024px) {
        .app-content {
            margin-left: 98px;
        }
    }
    .app-content:before {
        -webkit-transition: .3s;
        -o-transition: .3s;
        position: absolute;
        left: 0;
        width: calc(100vw - 98px);
        height: 230px;
        background: #02a8f3;
        content: "";
        opacity: 1;
        transition: .3s;
    }




    .badge {
        display: none;
    }
</style>
