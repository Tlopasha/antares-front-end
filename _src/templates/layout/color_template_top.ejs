<!-- Preloader -->

<style>
    [data-preload] {
        opacity: 0;
    }

    #app-wrapper.main-sidebar--top .main-menu.main-menu--primary > li a i, #app-wrapper.main-sidebar--top .main-menu.main-menu--secondary > li a i {
        margin-right: 16px;
    }

    .grid-stack-item-content {
        background: #fff;
    }

    @media only screen and (max-width: 1024px) {
        .app-content.page-dashboard {
            padding-bottom: 0;
            padding-top: 0;
        }
    }

    aside.main-sidebar {
        width: 100vw;
        height: 80px;
    }

    @media only screen and (max-width: 1023px) {
        aside.main-sidebar {
            transform: translateX(-100%);
        }
    }

    @media only screen and (min-width: 1024px) {
        aside.main-sidebar .nav-container {
            opacity: 0;
        }
    }

    .app-content:before {
        -webkit-transition: .3s;
        -o-transition: .3s;
        position: absolute;
        left: 0;
        width: 100%;
        height: 230px;
        background: #02a8f3;
        content: "";
        opacity: 1;
        transition: .3s;
    }

    #app-wrapper.main-sidebar--top .main-head, #app-wrapper.main-sidebar--top .main-content, #app-wrapper.main-sidebar--top .nav-container, #app-wrapper.main-sidebar--top .submenu section {
        max-width: 1400px;
        margin-left: auto;
        margin-right: auto;
        padding-left: 0;
        padding-right: 0;
    }

    #app-wrapper.main-sidebar--top .main-head--ca.main-head,
    #app-wrapper.main-sidebar--top.main-sidebar--ca .main-content,
    #app-wrapper.main-sidebar--top .main-menu-html--ca .nav-container,
    #app-wrapper.main-sidebar--top .main-menu-html--ca .submenu section {
        max-width: 1152px;
    }

    #app-wrapper.main-sidebar--top .main-menu-html--ca .more-trigger {
        display: none;
    }

    #app-wrapper.main-sidebar--top .main-menu-html--ca .client-area .item-grp-single:nth-child(2) {
        position: relative;
    }

    .badge {
        display: none;
    }
</style>
