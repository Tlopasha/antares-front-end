<!DOCTYPE html>
<html lang="en">
<% var pageTitle = 'Form Elements'; %>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="user-scalable=no, width=device-width, initial-scale=1, maximum-scale=1">
    <title><%= pageTitle %></title>
    <% if(mainMenuType === 'wide'){ %>
    <% include ../../layout/color_template_wide %>
    <% } else{ %>
    <% include ../../layout/color_template %>
    <% } %>
    <% include ../../layout/css_files %>
</head>

<body>
<% include ../../layout/app_wrapper_start_tag %>

<% if(clientArea){ %>
<% include ../../layout/mode_clientArea/CA_menutop %>
<% } else{ %>
<% include ../../layout/main_menu_html %>
<% } %>

<div class="app-content page-settings page-forms--hor">
    <% var breadData = 'Form Elements'; %>

    <% if(clientArea){ %>
    <% include ../../layout/mode_clientArea/CA_mainhead %>
    <% } else{ %>
    <% include ../../layout/mainhead %>
    <% } %>

    <% include ../../layout/sidebar_notifications %>
    <% include ../../layout/sidebar_alerts %>
    <section class="main-content" data-preload>
        <div class="grid-container grid-container--2col grid-container--footer grid-container--no-padd">
            <% include ../../layout/menu_aside_general_settings %>
            <div class="grid-col grid-col--container">
                <div class="menu-mobile-settings--box">
                    <select class="menu-mobile-settings" data-select2--class="menu-aside-settings--blue"
                            data-selectAR--search="true" name="select" id="select-anchor">
                    </select>
                </div>
                <section class="generals-form gen-settings--full">
                    <h1>Form Elements</h1>
                    <form>
                        <fieldset name="inputs">
                            <a name="AntaresInputs" id="AntaresInputs">
                                <h2>Inputs - simple</h2>
                            </a>
                            <% include ../ui_components/ui_input_simple %>
                        </fieldset>
                    </form>
                    <form>
                        <fieldset name="Selects">
                            <a name="AntaresSelects">
                                <h2>Selects</h2>
                            </a>
                            <% include ../ui_components/ui_selects %>
                        </fieldset>
                    </form>
                    <form>
                        <fieldset class="more-inputs">
                            <a name="AntaresRareInput">
                                <h2>More inputs</h2>
                            </a>
                            <% include ../ui_components/ui_more_input %>
                        </fieldset>
                    </form>
                    <form>
                        <fieldset name="Input-with-button">
                            <a name="AntaresInputWithButton">
                                <h2>Input with button</h2>
                            </a>
                            <% include ../ui_components/ui_input_with_btn %>
                        </fieldset>
                    </form>
                    <form>
                        <fieldset>
                            <a name="AntaresTextarea">
                                <h2>Textarea and tooltip</h2>
                            </a>

                            <% include ../ui_components/ui_textarea %>
                        </fieldset>
                    </form>

                    <form>
                        <fieldset name="AntaresAdditionals">
                            <a name="AntaresAdditionals">
                                <h2>Additionals</h2>
                            </a>
                            <% include ../ui_components/ui_additionals %>
                        </fieldset>
                    </form>

                    <form>
                        <fieldset name="RadioCheckSwitch">
                            <a name="AntaresSwitch">
                                <h2>Switch</h2>
                            </a>
                            <% include ../ui_components/ui_switch %>
                        </fieldset>
                    </form>
                    <form>
                        <fieldset name="RadioCheckSwitch">
                            <a name="AntaresRadio">
                                <h2>Radio</h2>
                            </a>
                            <% include ../ui_components/ui_radio %>
                        </fieldset>
                    </form>
                    <form>
                        <fieldset name="RadioCheckSwitch">
                            <a name="AntaresCheckbox">
                                <h2>Checkbox</h2>
                            </a>
                            <% include ../ui_components/ui_checkbox %>
                        </fieldset>
                    </form>
                    <form>
                        <fieldset name="Sliders" class="slider">
                            <a name="AntaresStandardSlider">
                                <h2>Slider</h2>
                            </a>
                            <% include ../ui_components/ui_sliders %>
                        </fieldset>
                    </form>
                    <form>
                        <fieldset name="spinners" class="spinner">
                            <a name="AntaresSpinner">
                                <h2>Spinners</h2>
                            </a>
                            <% include ../ui_components/ui_spinners %>
                        </fieldset>
                    </form>
                    <form>
                        <fieldset name="MDLForms">
                            <a name="AntaresMDForms">
                                <h2>MD Forms</h2>
                            </a>
                            <% include ../ui_components/ui_md_forms %>
                        </fieldset>
                    </form>
                    <form>
                        <fieldset name="inputFIle">
                            <a name="AntaresInputFile">
                                <h2>Input file</h2>
                            </a>
                            <% include ../ui_components/ui_input_file %>
                        </fieldset>
                    </form>

                    <% include ../../layout/app_footer %>
                </section>
            </div>
        </div>
    </section>
</div>
<% include ../../layout/js_files %>
<script>

</script>
</body>

</html>