<div class="form-block">
    <div class="col-group">
        <div class="col-mb-8 col-6">
            <label class="form-block__name">Company Name</label>
        </div>
        <div class="col-mb-16 col-18">
            <div class="input-field">
                <input type="text" placeholder="RS Studio">
            </div>
        </div>
    </div>
</div>
<div class="form-block">

    <div class="col-group">
        <div class="col-mb-8 col-6">
            <label class="form-block__name">Text read only</label>
        </div>
        <div class="col-mb-16 col-18">
            <div class="input-field">
                <input type="text" name="Text" placeholder="RS Studio" readonly
                       required>
            </div>
        </div>
    </div>
</div>
<div class="form-block">

    <div class="col-group">
        <div class="col-mb-8 col-6">
            <label class="form-block__name">Number</label>
        </div>
        <div class="col-mb-16 col-18">
            <div class="input-field">
                <input type="number" name="Number" placeholder="12345" required>
            </div>
        </div>
    </div>
</div>
<div class="form-block">

    <div class="col-group">
        <div class="col-mb-8 col-6">
            <label class="form-block__name">Password</label>
        </div>
        <div class="col-mb-16 col-18">
            <div class="input-field">
                <input type="password" name="Password" placeholder="RS Studio"
                       required>
            </div>
        </div>
    </div>
</div>
<div class="form-block">
    <div class="col-group">
        <div class="col-mb-8 col-6">
            <label class="form-block__name">Type error</label>
        </div>
        <div class="col-mb-16 col-18">
            <div class="input-field type-error">
                <input type="text" placeholder="RS Studio">
            </div>
        </div>
    </div>
</div>

<div class="form-block">
    <div class="col-group">
        <div class="col-mb-8 col-6">
            <label class="form-block__name">Type warning</label>
        </div>
        <div class="col-mb-16 col-18">
            <div class="input-field type-warning">
                <input type="text" placeholder="RS Studio">
            </div>
        </div>
    </div>
</div>

<div class="form-block">
    <div class="col-group">
        <div class="col-mb-8 col-6">
            <label class="form-block__name">Type success</label>
        </div>
        <div class="col-mb-16 col-18">
            <div class="input-field type-success">
                <input type="text" placeholder="RS Studio">
            </div>
        </div>
    </div>
</div>

<div class="form-block">
    <div class="col-group">
        <div class="col-mb-8 col-6">
            <label class="form-block__name">Type disabled</label>
        </div>
        <div class="col-mb-16 col-18">
            <div class="input-field  type--disabled">
                <input type="text" placeholder="RS Studio" disabled>
            </div>
        </div>
    </div>
</div>
