  "browserslist": {
    "production": [
      "last 4 version",
      "ie 11"
    ],
    "development": [
      "last 1 version"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb",
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "jquery": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "jquery": true,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "linebreak-style": 0,
      "max-len": "off",
      "semi": 2,
      "indent": [
        "error",
        2
      ],
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true
        }
      ],
      "space-before-function-paren": [
        "error",
        "never"
      ],
      "comma-dangle": [
        "error",
        "never"
      ],
      "func-names": [
        "error",
        "never"
      ],
      "no-unused-vars": 0
    },
    "plugins": [
      "prettier"
    ]
  },
  "babel": {
    "plugins": [
      "syntax-dynamic-import"
    ],
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "safari >= 7",
              "ie >= 11",
              "edge >= 14"
            ]
          }
        }
      ]
    ]
  }
